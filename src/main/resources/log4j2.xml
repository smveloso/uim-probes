<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn">
  <Appenders>
    <Console name="CONSOLE" target="SYSTEM_OUT">
      <PatternLayout pattern="%d %-5p [%t] %C{2} (%F:%L) - %m%n"/>
    </Console>
    <File name="FILE" fileName="trt3jboss.log">
      <PatternLayout pattern="%d %-5p [%t] %C{2} (%F:%L) - %m%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="warn">
      <AppenderRef ref="CONSOLE"/>
    </Root>
    <Logger name="br.jus.trt3.seit.uim.probe" level="debug" additivity="false">
      <AppenderRef ref="FILE"/>
    </Logger>
  </Loggers>
</Configuration>