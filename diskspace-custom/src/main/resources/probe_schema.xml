<probe-schema probe-name="probe-name-string">
    <element-types>
        <element-type name="RESOURCE">
            <qos-metric-types>
                <qos-metric-type name="ResourceResponseTime">
                    <description>Duration time in milli-seconds of last resource collection cycle.</description>
                    <label>ResourceResponseTime</label>
                    <metric-type>1.10:14</metric-type>
                    <qos-name>QOS_RESOURCE_RESPONSE_TIME</qos-name>
                    <unit>MilliSeconds</unit>
                </qos-metric-type>
            </qos-metric-types>
        </element-type>

        <element-type name="StorageDirectory">
            <base-element-type>FileElement</base-element-type>
            <properties/>
            <qos-metric-types>
                <qos-metric-type name="DfUsagePercentage">
                    <active>yes</active>
                    <description>Directory used space (%)</description>
                    <label>UsedSpacePercentage</label>
                    <metric-type>1.1:3</metric-type>
                    <qos-name>QOS_STORAGE_DIRECTORY_USAGE_PCT</qos-name>
                    <unit>pct</unit>
                    <conversions/>
                </qos-metric-type>
                <qos-metric-type name="DfFreePercentage">
                    <active>yes</active>
                    <description>Directory free space (%)</description>
                    <label>FreeSpacePercentage</label>
                    <metric-type>1.1:14</metric-type>
                    <qos-name>QOS_STORAGE_DIRECTORY_FREE_PCT</qos-name>
                    <unit>pct</unit>
                    <conversions/>
                </qos-metric-type>                
            </qos-metric-types>
        </element-type>
    </element-types>
</probe-schema>

